---
import { Image } from 'astro:assets'
import oneTimeHeroImage from '../assets/images/hero/one-time.png'
import subscriptionImage from '../assets/images/hero/subscription.png'
import Marquee from './Marquee.astro'
---

<section class='relative flex h-max w-full flex-col gap-36'>
  <div class='flex flex-col gap-8 px-6'>
    <h1
      class='mx-auto max-w-[14ch] pb-[7.4em] text-center leading-[1.05] tracking-tighter'
    >
      Halver helps you split
      <span class='relative inline-block text-[--text-casal]'>
        one-time payments
        <Image
          aria-hidden
          class='hero-image pointer-events-none absolute max-w-[110%] select-none'
          src={oneTimeHeroImage}
          loading='eager'
          alt='A group of ladies at lunch.'
        />
      </span>
       and <span class='relative inline-block text-[--text-apricot]'>
        subscriptions
        <Image
          aria-hidden
          class='hero-image pointer-events-none absolute select-none'
          src={subscriptionImage}
          loading='eager'
          alt='A group of friends watching subscription TV.'
        />
      </span> with ease
    </h1>

    <div class='relative mx-auto flex flex-wrap justify-center gap-4'>
      <p class='sr-only'>Coming soon to the stores</p>

