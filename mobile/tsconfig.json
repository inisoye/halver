{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    /* Path alias config */ "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
